[user]
	name = Joe Cucci
	email = jcucci@fastmail.com
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[core]
    pager = delta
[interactive]
    diffFilter = delta --color-only
[alias]
	unstate = reset HEAD --
	last = log -1 HEAD
	unstage = reset HEAD --
[include]
    path = /home/jcucci/projects/system/catppuccin/delta/catppuccin.gitconfig
[delta]
    features = catppuccin-mocha
    navigate = true
    side-by-side = true
    line-numbers = true
[merge]
    conflictstyle = zdiff3
[pull]
	rebase = false
